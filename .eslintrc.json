{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier",
        "next"
    ],
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "@next/next/no-img-element": "off",
        "@typescript-eslint/explicit-module-boundary-types": "off",
        "no-undef": "off",
        "semi": "warn",
        "eqeqeq": "warn",
        "curly": "warn"
    }
}